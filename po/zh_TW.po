msgid "rm [-r] [-f] <files>"
msgstr "rm [-r] [-f] <檔案>"

#: src/commands.cc:421
msgid ""
"Remove remote files\n"
" -r  recursive directory removal, be careful\n"
" -f  work quietly\n"
msgstr ""
"刪除遠端檔案\n"
" -r  同時刪除所有子目錄，請小心使用\n"
" -f  不輸出任何結果\n"

