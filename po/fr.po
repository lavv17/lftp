msgid "source <file>"
msgstr "source <fichier>"

#: src/commands.cc:457
msgid "Execute commands recorded in file <file>\n"
msgstr "Exécute les commandes enregistrées dans le fichier <fichier>\n"

#: src/commands.cc:459
