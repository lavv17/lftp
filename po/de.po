msgstr "Benutzung: %s lokales-Verzeichnis\n"

#: src/commands.cc:449
#, c-format
msgid "lcd ok, local cwd=%s\n"
msgstr "lcd OK, lokales cwd=%s\n"

#: src/commands.cc:463
#, c-format
